<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.InsertPackageController">
   <center>
      <TitledPane alignment="CENTER" animated="false" collapsible="false" prefHeight="146.0" prefWidth="252.0" text="Create Package" BorderPane.alignment="CENTER">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="250.0">
               <children>
                  <Label layoutX="86.0" layoutY="6.0" text="Package Name" />
                  <TextField fx:id="packageName" layoutX="51.0" layoutY="23.0" />
                  <Button fx:id="insertButton" layoutX="100.0" layoutY="65.0" mnemonicParsing="false" onAction="#handleButtonSave" text="Save" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </center>
</BorderPane>
